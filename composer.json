{
    "authors": [
        {
            "name": "Omni Adams",
            "email": "omni@digitaldarkness.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Commlink\\": "/mnt/sys/www/commlink/api/classes",
            "RollBot\\": "src"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "description": "Dice roller for Slack",
    "license": "MIT",
    "minimum-stability": "stable",
    "name": "omnicolor/dice-roller",
    "prefer-stable": true,
    "require": {
        "predis/predis": "^1.1",
        "mongodb/mongodb": "^1.2",
        "lcobucci/jwt": "^3.2",
        "guzzlehttp/guzzle": "^6.3",
        "monolog/monolog": "^2.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.19",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "scripts": {
        "all": [
            "@phpstan",
            "@cs"
        ],
        "cs": "phpcs",
        "phpstan": "phpstan analyze"
    },
    "type": "project"
}
